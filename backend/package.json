{
  "name": "hyperskills",
  "version": "0.0.1",
  "description": "Mini Product designed to improve my productivity",
  "main": "./dist/server.js",
  "scripts": {
    "test": "jest",
    "dev": "nodemon --exec ts-node -- ./server.ts",
    "build": "tsc",
    "start": "node ./dist/server.js",
    "db:up": "docker-compose up -d",
    "migration-reverse": "ts-node ./lib/buildModels.ts",
    "migration-reverse-2": "typeorm-model-generator -h 127.0.0.1 -d hyperskills -p 5432 -u postgres -x root -e postgres --output entities",
    "db-dump-heroku": "heroku pg:backups:capture --app hyperskills && heroku pg:backups:url b030 --app hyperskills",
    "db-restore-from-heroku": "pg_restore --no-owner -h localhost -U postgres --clean --dbname=hyperskills databases/hyperskills-heroku-backup.dump",
    "db-dump": "pg_dump -Fc --no-acl --no-owner -h localhost -U postgres hyperskills >  /hyperskills.dump",
    "db-restore-local": "pg_restore --no-owner -h localhost -U postgres --clean -d hyperskills databases/hyperskills-heroku-backup.dump",
    "db-restore-heroku": "heroku pg:backups:restore '<SIGNED URL>' DATABASE_URL",
    "db-backup": "heroku pg:backups:capture --app hyperskills && heroku pg:backups:url b034 --app hyperskills"
  },
  "engines": {
    "node": "13.5.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EddyArellanes/hyperskills-reloaded.git"
  },
  "keywords": [
    "framework",
    "backend",
    "nodejs",
    "expressjs",
    "jwt"
  ],
  "author": "Eddy Arellanes Bastida",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EddyArellanes/hyperskills-reloaded.gitissues"
  },
  "homepage": "https://github.com/EddyArellanes/hyperskills-reloaded.git#readme",
  "dependencies": {
    "@types/bluebird": "^3.5.30",
    "@types/node": "^10.17.18",
    "@types/validator": "^12.0.1",
    "axios": "^0.19.0",
    "cors": "^2.8.5",
    "cron": "^1.7.2",
    "crypto-js": "^3.1.9-1",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-validator": "^6.2.0",
    "firebase-admin": "^8.7.0",
    "googleapis": "^43.0.0",
    "heroku-ssl-redirect": "0.0.4",
    "jsonwebtoken": "^8.4.0",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "node-nlp": "^3.10.2",
    "nodemailer": "^6.3.1",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.21.6",
    "sequelize-typescript": "^1.1.0",
    "sequelize-typescript-generator": "^2.1.0",
    "typeorm": "^0.2.24",
    "typeorm-model-generator": "^0.4.2",
    "uuidv4": "^5.0.1",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "_moduleAliases": {
    "@lib": "dist/lib/"
  }
}
